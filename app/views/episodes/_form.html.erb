<%= form_for ([@podcast, @episode]) do |f| %>

	<div class="field">	
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</div>

	<div class="field">		
		<%= f.label :description %><br>
		<%= f.text_field :description %>
	</div>

	<%= f.submit class:"button button_highlight button_block" %>
<% end %>